<% @movie.errors.full_messages.each do |error| %>
  <%= error %><br>
  <% end %>

<%= form_for @movie do |f| %>

<%= f.label :title %>
<%= f.text_field :title %><br>

<%= f.label :director %>
<%= f.text_field :director %><br>

<%= f.label :description %>
<%= f.text_area :description %><br>

<%= f.submit %>
<% end %>